{
  "name": "dersonsena/yii2-jwt-tools",
  "type": "library",
  "description": "",
  "keywords": [],
  "scripts": {
    "php-cs": "phpcs ./src"
  },
  "authors": [
    {
      "name": "Kilderson Sena",
      "email": "dersonsena@gmail.com",
      "role": "Developer"
    }
